<!DOCTYPE html>

<title>Hello AdminController!</title>

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
    table { background: #2196F3; color:  white; border-collapse: collapse; }
    td, th {border: solid white; }
</style>

<div class="example-wrapper">
    <h1>Hello {{ controller_name }}! âœ…</h1>
</div>
<div id="data">
    {{ form_start(user_form) }}
    <div class="my-custom-class-for-errors">
        {{ form_errors(user_form) }}
    </div>
    <div>
        {{ form_row(user_form.email) }}
    </div>
    <div>
        {{ form_row(user_form.meno) }}
    </div>
    <div>
        {{ form_row(user_form.priezvisko) }}       
    </div>
    {{ form_end(user_form) }}
</div>
{% endblock %}